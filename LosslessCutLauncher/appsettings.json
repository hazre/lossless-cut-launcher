      "Windows": {
        "DownloadUrlFormat": "https://github.com/mifi/lossless-cut/releases/download/{0}/LosslessCut-win-x64.7z",
        "ExecutableName": "LosslessCut.exe"
      },
      "Linux": {
        "DownloadUrlFormat": "https://github.com/mifi/lossless-cut/releases/download/{0}/LosslessCut-linux-x64.tar.bz2",
        "ExecutableName": "LosslessCut"
      }
    }
  },
  "SelfUpdate": {
